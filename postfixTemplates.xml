    </template>
    <template id="toboo@userDefined" key=".toboo" provider="builtin.js" language-level="ES6" topmost="false">
      <conditions />
      <template name="fakeKey" value="Boolean($EXPRS$)" toReformat="true" toShortenFQNames="true" />
    </template>
  </component>
</application>