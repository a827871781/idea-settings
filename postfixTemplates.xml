      <conditions />
      <template name="fakeKey" value="$EXPRS$+''" toReformat="true" toShortenFQNames="true" />
    </template>
    <template id="toInt@userDefined" key=".tonum" provider="builtin.js" language-level="ES5" topmost="true">
      <conditions />
